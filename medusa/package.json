    "@mikro-orm/cli": "6.4.16",
    "@react-email/preview-server": "^5.2.8",
    "@swc/core": "^1.15.11",
    "@swc/jest": "^0.2.39",
    "@tanstack/eslint-plugin-query": "5.91.4",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.19.11",
    "@types/react": "^18.3.28",
